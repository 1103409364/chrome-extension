{
	"name": "vite-vue3-windicss-chrome-extension-starter",
	"version": "0.0.0",
	"keyword": "Chrome, Chrome extension, Vite, Vue, Vue3, Windi CSS",
	"watch": {
		"build": {
			"patterns": [
				"src"
			],
			"extensions": "ts,vue, js,html",
			"runOnChangeOnly": false
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build && pnpm copy",
		"watch": "npm-watch",
		"watchs": "vite build --watch",
		"serve": "vite preview",
		"copy": "node ./scripts/copyAssets.js",
		"reinstall": "rm -rf node_modules && pnpm install"
	},
	"dependencies": {
		"vue": "^3.2.16"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"@vitejs/plugin-legacy": "^1.6.2",
		"@vitejs/plugin-vue": "^1.9.3",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.9.0",
		"glob": "^7.2.0",
		"npm-watch": "^0.11.0",
		"prettier": "^2.8.3",
		"vite": "^2.6.4",
		"vite-plugin-compression": "^0.3.5",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-imagemin": "^0.4.6",
		"vite-plugin-windicss": "^1.5.1",
		"windicss": "^3.2.1",
		"postcss-html": "^1.5.0",
		"postcss-scss": "^4.0.6",
		"sass": "1.3.2",
		"sass-loader": "^10.3.1",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recess-order": "^3.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard-scss": "^3.0.0"
	}
}
